# Shortcuts to the clipboard
alias wcopy="pbcopy"
alias wpaste="pbpaste"

# useful shortcuts
alias open="nautilus"

# Shortcuts

# useful shortcuts
alias md5="md5sum"

# Alias vi to vim
if type vim &> /dev/null; then
	alias vi="vim"
fi

# Alias clamscan with avscan
if type clamscan &> /dev/null; then
	alias avscan="clamscan"
fi
